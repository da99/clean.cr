      "body" : {
        "margin": "0 auto",
        "width": "800px"
      }
    }
  },

  "markup" : [
    {"page-title": "Hello, Test 00"},
    {"p": null, "body": "main.msg1"},
    {"p": "main.msg2"},
    {"div": null, "class":"column", "body": [
      {"span": "msg1"},
      {"span": "msg2"}
    ]},
    {"each" : "names", "as": "name", "body": [
      {"p": "name"}
    ]},
    {"footer": null, "body": "{{copyright}}"},
    {"input": "msg2", "name": "my-hidden", "type": "hidden" }
  ]

}
